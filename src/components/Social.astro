---
import { Icon } from "astro-icon";

interface Props {
  href: string;
  iconPath: string;
}

const { href, iconPath } = Astro.props;

import IconX from "../icons/socials/x.svg";
---

<div>
  <a
    class="md:w-[35px] w-[28px] flex justify-center items-center p-1.5 bg-customBlue rounded-full"
    target="_blank"
    href={href}
  >
    <img src={IconX} alt="social" style="fill: white;" />
  </a>
</div>
